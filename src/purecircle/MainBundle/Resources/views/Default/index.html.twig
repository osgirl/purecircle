{% endblock %}

{% block stylesheets %}

    {{parent()}}


    <!-- Bootstrap core CSS -->
    


    {% stylesheets
                
               filter="cssrewrite" 
    %}

    <link  rel="stylesheet" href="{{asset_url}}"  />
    {% endstylesheets %}    

{% endblock %}
{% block body %}
    <!--header start -->


    <section id="container" >
        <!--header start-->
        <header class="header white-bg">
            <div class="sidebar-toggle-box">
                <div data-original-title="" data-placement="right">P|c</div>
                
            </div>
            <!--logo start-->
            <a href="{{path('purecircle_main')}}" class="logo" >Pure<span>Circle</span></a>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
                <!--  notification start -->
                <ul class="nav top-menu">
                    <!-- settings start -->
                    
                   
            </div>
            <div class="top-nav ">
                <ul class="nav pull-left top-menu">
                   
{#              check for authentication for button to show     #}
                 {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
    <a class="link" href="{{ path('purecircle_dash') }}"><button  class="btn btn-success">< < Dasboard</button></a>
{% else %}
 
{% endif %}
                    
                    
                </ul>
            </div>
            <div class="top-nav ">
                <ul class="nav pull-right top-menu">
                   
{#              check for authentication for button to show     #}
                 {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
    <a class="link" href="{{ path('purecircle_logout') }}"><button  class="btn btn-warning">Logout</button></a>
{% else %}
   <a href="{{path('purecircle_login')}}"> <button  class="btn btn-danger">Login </button></a>

{% endif %}
                    
                    
                </ul>
            </div>
        </header>
        <!--header end-->
       
       <section id="content">
