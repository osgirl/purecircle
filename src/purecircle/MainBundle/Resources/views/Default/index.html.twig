        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                {{ "now"|date("Y") }} &copy; P|c by Image-Ad.
                <a href="#" class="go-top">
                    <i class="icon-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->
    </section>










{% endblock %}


