{% extends 'base.html.twig' %}
{% block title %}
    Purecircle Report
{% endblock %}

{% block stylesheets %}

    {{parent()}}


    <!-- Bootstrap core CSS -->
    


    {% stylesheets
                
               filter="cssrewrite" 
    %}

    <link  rel="stylesheet" href="{{asset_url}}"  />
    {% endstylesheets %}    

{% endblock %}
{% block body %}
    <!--header start -->


    <section id="container" >
        <!--header start-->
        <header class="header white-bg">
            <div class="sidebar-toggle-box">
                <div data-original-title="" data-placement="right">P|c</div>
                
            </div>
            <!--logo start-->
            <a href="{{path('purecircle_main')}}" class="logo" >Pure<span>Circle</span></a>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
                <!--  notification start -->
                <ul class="nav top-menu">
                    <!-- settings start -->
                    
                   
            </div>
            <div class="top-nav ">
                <ul class="nav pull-right top-menu">
                   
                    <!-- user login dropdown start-->
                    
                    <a href="{{path('purecircle_login')}}"> <button  class="btn btn-danger">Login </button></a>
                    
                </ul>
            </div>
        </header>
        <!--header end-->
       
       <section id="content">
           <iframe width='100%' height='500px' style="margin-top:50px" frameBorder='0' src='https://a.tiles.mapbox.com/v4/paulem.lkf89bg7/attribution,zoompan,geocoder.html?access_token=pk.eyJ1IjoicGF1bGVtIiwiYSI6InNPUW9ta1EifQ.gzjp8DeHb8Q5H3QUX-A22Q'></iframe></section>
        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                {{ "now"|date("Y") }} &copy; P|c by Image-Ad.
                <a href="#" class="go-top">
                    <i class="icon-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->
    </section>










{% endblock %}


{% block javascripts %}

    <!-- js placed at the end of the document so the pages load faster -->
    <script src="{{asset('bundles/purecirclemain/js/jquery.js')}}"></script>
    <script src="{{asset('bundles/purecirclemain/js/bootstrap.min.js')}}"></script>
    <script class="include" type="text/javascript" src="{{asset('bundles/purecirclemain/js/jquery.dcjqaccordion.2.7.js')}}"></script>
    <script src="{{asset('bundles/purecirclemain/js/jquery.scrollTo.min.js')}}"></script>
    <script src="{{asset('bundles/purecirclemain/js/jquery.nicescroll.js')}}" type="text/javascript"></script>
    <script src="{{asset('bundles/purecirclemain/js/respond.min.js')}}" ></script>

    <!--common script for all pages-->
   
    
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
   

   
 <script src="{{asset('bundles/purecirclemain/js/common-scripts.js')}}"></script>
    



    {%javascripts 
          
    %} 




    {%endjavascripts %} 

{% endblock %}



