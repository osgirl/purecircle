<!DOCTYPE html>
<html style="overflow: hidden;">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        {% block stylesheets %}
            <!-- Bootstrap core CSS -->
    <link type="text/css" href="{{asset('bundles/purecirclemain/css/bootstrap.min.css')}}" rel="stylesheet">
    <link type="text/css" href="{{asset('bundles/purecirclemain/css/bootstrap-reset.css')}}" rel="stylesheet">
    <!--external css-->
    <link type="text/css" href="{{asset('bundles/purecirclemain/assets/font-awesome/css/font-awesome.css')}}" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link  type="text/css" href="{{asset('bundles/purecirclemain/css/style.css')}}" rel="stylesheet">

        
         {% stylesheets
            
                
               filter="cssrewrite" 
         %}

         <link  rel="stylesheet" href="{{asset_url}}"  />
             {% endstylesheets %}    
          
        
        
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
     <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="{{asset('bundles/purecirclemain/js/html5shiv.js')}}"></script>
      <script src="{{asset('bundles/purecirclemain/js/respond.min.js')}}"></script>
    <![endif]-->
    </head>
    <body>
        {% block body %}{% endblock %}
        
        {% block javascripts %}
        
        
        <!-- js placed at the end of the document so the pages load faster -->
    <script src="{{asset('bundles/purecirclemain/js/jquery.js')}}"></script>
    <script src="{{asset('bundles/purecirclemain/js/bootstrap.min.js')}}"></script>
    <script class="include" type="text/javascript" src="{{asset('bundles/purecirclemain/js/jquery.dcjqaccordion.2.7.js')}}"></script>
    <script src="{{asset('bundles/purecirclemain/js/jquery.scrollTo.min.js')}}"></script>
    <script src="{{asset('bundles/purecirclemain/js/jquery.nicescroll.js')}}" type="text/javascript"></script>
    <script src="{{asset('bundles/purecirclemain/js/respond.min.js')}}" ></script>

    <!--common script for all pages-->
   
    
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
   

   
 <script src="{{asset('bundles/purecirclemain/js/common-scripts.js')}}"></script>
        
        {% endblock %}
    </body>
</html>
